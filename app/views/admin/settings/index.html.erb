<h1>Listing admin_settings</h1>

<table>
  <tr>
    <th>Assertion consumer service url</th>
    <th>Assertion consumer service binding</th>
    <th>Single Logout service url</th>
    <th>Single Logout service binding</th>
    <th>Idp metadata</th>
    <th>Idp metadata ttl</th>
    <th>Name identifier format</th>
    <th>Issuer</th>
    <th>Authn context</th>
    <th>Idp cert fingerprint</th>
    <th>Idp sso target url</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @admin_settings.each do |admin_setting| %>
  <tr>
    <td><%= admin_setting.assertion_consumer_service_url %></td>
    <td><%= admin_setting.assertion_consumer_service_binding %></td>
    <td><%= admin_setting.single_logout_service_url %></td>
    <td><%= admin_setting.single_logout_service_binding %></td>
    <td><%= admin_setting.idp_metadata %></td>
    <td><%= admin_setting.idp_metadata_ttl %></td>
    <td><%= admin_setting.name_identifier_format %></td>
    <td><%= admin_setting.issuer %></td>
    <td><%= admin_setting.authn_context %></td>
    <td><%= admin_setting.idp_cert_fingerprint %></td>
    <td><%= admin_setting.idp_sso_target_url %></td>
    <td><%= link_to 'Show', admin_setting %></td>
    <td><%= link_to 'Edit', edit_admin_setting_path(admin_setting) %></td>
    <td><%= link_to 'Destroy', admin_setting, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Setting', new_admin_setting_path %>
